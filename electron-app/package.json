{
  "name": "electron-app",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "dev": "concurrently \"vite --config src/vite.config.ts\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development electron ./dist-electron/main.cjs\"",
    "build": "vite build --config src/vite.config.ts && npm run build:electron",
    "start": "electron .",
    "build:electron": "del dist-electron\\main.cjs dist-electron\\preload.cjs 2>nul && tsc -p tsconfig.electron.json && ren dist-electron\\main.js main.cjs && ren dist-electron\\preload.js preload.cjs",
    "start:electron": "npm run build:electron && electron dist-electron/main.cjs"
  },
  "main": "electron/main.ts",
  "dependencies": {
    "@tailwindcss/vite": "^4.1.8",
    "@types/react-router-dom": "^5.3.3",
    "lucide-react": "^0.513.0",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "tailwindcss": "^4.1.8"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@types/sqlite3": "^3.1.11",
    "@vitejs/plugin-react": "^4.4.1",
    "concurrently": "^9.1.2",
    "cross-env": "^7.0.3",
    "electron": "^36.3.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5",
    "wait-on": "^8.0.3"
  }
}
